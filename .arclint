{
  "exclude": ["(^dist/)", "(env/)", "(venv/)"],
  "linters": {
    "filename": {
	  "type": "filename"
	},
	
	"spelling": {
		"type": "spelling"
	},
	
	"generated": {
	  "type": "generated"
	},
	
	"merge-conflict": {
	  "type": "merge-conflict"
	},
	
	"chmod": {
	  "type": "chmod"
	},
	
    "python": {
      "type": "pep8",
      "include": "(\\.py$)",
	  "exclude": "(test.*.py)",
	  "severity": {
		"E501": "disabled",
		"E402": "disabled"
      }
    }
  }
}